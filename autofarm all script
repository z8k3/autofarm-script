i:Label("Kung Fu Simulator",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
})

i:Toggle("autofarm (need to be in range of punching bag)",function(bool)
    shared.toggle = bool
    while wait() do
        local args = {
            [1] = workspace:WaitForChild("Targets"):WaitForChild("TargetsZone1"):WaitForChild("skill"):WaitForChild("1"):WaitForChild("Cylinder")
        }
        game:GetService("ReplicatedStorage"):WaitForChild("WeaponSystem"):WaitForChild("Remotes"):WaitForChild("DummyHit"):FireServer(unpack(args))
    end
end)

i:Toggle("autofarm for second bag",function(bool)
    shared.toggle = bool
    while wait() do

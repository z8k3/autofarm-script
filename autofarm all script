-- scripts to use for script making, dark dex, simple spy, hydroxide if u dont like simple spy, and x ghost hub cuz i found a position reader (might use for tp)
-- not the best BUT its my first autofarm script (might add some more)

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("Zek's scripts") -- Creates the window

local b = w:CreateFolder("runstar autofarm") -- Creates the folder(U will put here your buttons,etc)

local c = w:CreateFolder("boxer clicker autofarm")

local d = w:CreateFolder("dead lifting autofarm")

local e = w:CreateFolder("ninja legends autofarm")

local f = w:CreateFolder("strongest punch sim autofarm")

local g = w:CreateFolder("Power Slap autofarm")

local a = w:CreateFolder("Ninja Fighting autofarm")

local h = w:CreateFolder("Tug Of War autofarm")

local i = w:CreateFolder("Kung Fu Simulator")

local j = w:CreateFolder("Undertale Judgement Day")

b:Label("runstar autofarm",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
}) 


b:Toggle("Toggle",function(bool)
    shared.toggle = bool
    while wait() do
        local args = {
            [1] = "20" --rejoin and change value according to number of treadmill if u wanna get better speed
        }
    
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Train"):WaitForChild("AddSpeed"):FireServer(unpack(args))
    end

end)

b:Toggle("AutoRaceClick (might not work)",function(bool)
    shared.toggle = bool
    
    --MIGHT NOT WORK
    while wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Race"):WaitForChild("RaceClick"):InvokeServer()
    end

end)


b:DestroyGui()

---------------------------------------------------------------------------------------

c:Label("boxer clicker autofarm",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

c:Toggle("Toggle",function(bool)
    shared.toggle = bool
    
    while wait() do
        local args = {
            [1] = 1
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Train"):WaitForChild("AddArm"):FireServer(unpack(args))
    end

end)

---------------------------------------------------------------------------------------

